{
  "name": "acrrm-curriculum-docusaurus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf .docusaurus/ build/",
    "start": "docusaurus start",
    "build": "yarn run clean && docusaurus build",
    "serve": "yarn build && npx http-server build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "move-pdf": "shx mv docusaurus.pdf build/generated.pdf && shx cp build/generated.pdf static/generated.pdf",
    "create-pdf": "npx docusaurus-pdf from-build-config && yarn run move-pdf"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.58",
    "@docusaurus/preset-classic": "^2.0.0-alpha.58",
    "clsx": "^1.1.1",
    "docusaurus-lunr-search": "^2.0.1",
    "docusaurus-plugin-auto-sidebars": "^1.0.5",
    "netlify-cms": "^2.10.51",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "shx": "^0.3.2"
  }
}
